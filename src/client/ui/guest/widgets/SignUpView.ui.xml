<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <g:VerticalPanel styleName="center margintop fakefakefakeBtn">
            <g:Label>Email</g:Label>
            <g:TextBox ui:field="emailField"></g:TextBox>
            <g:Label>Name</g:Label>
            <g:TextBox ui:field="nameField"></g:TextBox>
            <g:Label>Cyklist type</g:Label>
            <g:ListBox ui:field="cyclistTypeList">
                <g:item value="master">master</g:item>
                <g:item value="enthusiast">enthusiast</g:item>
                <g:item value="cruiser">cruiser</g:item>
                <g:item value="debutant">debutant</g:item>
            </g:ListBox>
            <g:Label>Firma</g:Label>
            <g:ListBox ui:field="firmList">
            </g:ListBox>
            <g:Label>Password</g:Label>
            <g:PasswordTextBox ui:field="passwordField"></g:PasswordTextBox>
            <g:Label>Password again</g:Label>
            <g:PasswordTextBox ui:field="passwordCheckField"></g:PasswordTextBox>
            <g:Button ui:field="signUpBtn">Sign Up</g:Button>
        </g:VerticalPanel>

        <g:VerticalPanel styleName="center">
            <g:HTML ui:field="errorMessageLabel"></g:HTML>
        </g:VerticalPanel>

    </g:HTMLPanel>
</ui:UiBinder>