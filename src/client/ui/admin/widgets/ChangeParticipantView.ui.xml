<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'>
    <g:HTMLPanel>
        <g:VerticalPanel styleName="center">
            <g:Label ui:field="idLabel"></g:Label>

            <g:Label>Navn</g:Label>
            <g:TextBox ui:field="nameField"></g:TextBox>
            <g:Label >Email</g:Label>
            <g:TextBox ui:field="emailField"></g:TextBox>
            <g:Label >Person-type</g:Label>
            <g:ListBox ui:field="personTypeList">
                <g:item value="TEAMCAPTAIN">TEAMCAPTAIN</g:item>
                <g:item value="PARTICIPANT">PARTICIPANT</g:item>
                <g:item value="ADMIN">ADMIN</g:item>
                <g:item value=""></g:item>
            </g:ListBox>
            <g:Label >Cyclist-type</g:Label>
            <g:ListBox ui:field="cyclistTypeList">
                <g:item value="master">master</g:item>
                <g:item value="enthusiast">enthusiast</g:item>
                <g:item value="cruiser">cruiser</g:item>
                <g:item value="debutant">debutant</g:item>
                <g:item value=""></g:item>
            </g:ListBox>
            <g:Label >Password</g:Label>
            <g:PasswordTextBox ui:field="passField"></g:PasswordTextBox>
            <g:Label>Firma</g:Label>
            <g:TextBox ui:field="firmNameField"></g:TextBox>
            <g:Label>Hold</g:Label>
            <g:TextBox ui:field="teamNameField"></g:TextBox>
            <g:Button ui:field="submitBtn">Submit</g:Button>
            <g:Button ui:field="returnBtn">GÃ¥ tilbage</g:Button>
            <g:Button ui:field="deleteBtn">Slet deltager</g:Button>
        </g:VerticalPanel>
    </g:HTMLPanel>
</ui:UiBinder>